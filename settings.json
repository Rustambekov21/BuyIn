{
        "git.enableSmartCommit": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnType": true,
        "prettier.tabWidth": 4,
        "prettier.jsxSingleQuote": true,
        // "prettier.semi": false,
        "prettier.signlQeuote": true,
        "workbench.iconTheme": "material-icon-theme",
        "git.autofetch": true,
        "javascript.updateImportsOnFileMove.enabled": "always",
        "window.enableMenuBarMnemonics": false,
        "terminal.integrated.defaultProfile.linux": "bash",
        "git.confirmSync": false,
        "liveServer.settings.donotVerifyTags": true,
        "workbench.colorTheme": "Default Dark+",
        "liveSassCompile.settings.autoprefix": [],
        "liveSassCompile.settings.formats": [
            {
                "format": "expanded",
                "extensionName": ".css",
                "savePath": "./css"
            }
        ],
        "code-runner.runInTerminal": true,
        "ecsstractor_port.bem_nesting": true,
        "editor.cursorBlinking": "smooth",
        "powermode.combo.counterEnabled": "hide",
        "powermode.combo.counterSize": 10,
        "powermode.combo.timeout": 100,
        "powermode.explosions.offset": 0,
        "powermode.explosions.size": 3,
        "powermode.enabled": true,
        "powermode.shake.enabled": false,
        "powermode.combo.threshold": 0.7,
        "powermode.combo.timerEnabled": "hide",
        "powermode.shake.intensity": 8,
        "typescript.updateImportsOnFileMove.enabled": "always",
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 100,
        "editor.cursorSmoothCaretAnimation": "on",
        "editor.guides.bracketPairs": "active",
        "vscode-pets.theme": "beach",
        "vscode-pets.petColor": "red",
        "vscode-pets.petType": "chicken",
        "liveServer.settings.donotShowInfoMsg": true,
        "editor.minimap.enabled": false,
        "terminal.integrated.enableMultiLinePasteWarning": false,
        "terminal.integrated.defaultProfile.windows": "Git Bash",
        "files.autoSave": "afterDelay",
        "prettier.bracketSameLine": true,
        "editor.formatOnPaste": true,
        "window.zoomLevel": 2
 
}